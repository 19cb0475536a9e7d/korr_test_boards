if ARCH_BOARD_KORR_TEST_SIGNAL_BOARD

config KORR_TEST_SIGNAL_BOARD_USER_LED_DEVICE
	bool "User LED device driver"
	default n
	---help---
		Enables creation of a /dev/led device

config KORR_TEST_SIGNAL_BOARD_USER_LED_DEVICE_NUMBER
	int "Index number of user leds in /dev/led"
	default 0
	depends on KORR_TEST_SIGNAL_BOARD_USER_LED_DEVICE
	---help---
		Specifies the index number of the user leds.
	
endif
